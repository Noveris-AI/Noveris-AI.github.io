# PostgreSQL Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=relationship_repair
POSTGRES_PORT=8880

# Redis Configuration
REDIS_PASSWORD=your_redis_password_here
REDIS_PORT=6379

# PgAdmin (PostgreSQL Management UI - optional)
PGADMIN_EMAIL=admin@example.com
PGADMIN_PASSWORD=your_pgadmin_password_here
PGADMIN_PORT=5050

# Redis Commander (Redis Management UI - optional)
REDIS_COMMANDER_PORT=8081

# Application Configuration (for reference)
# These should be set in your main .env or .env.local file
DATABASE_URL="postgresql://postgres:your_secure_password_here@localhost:5432/relationship_repair?schema=public"
REDIS_URL="redis://localhost:6379"

# AI Provider (Configure at least one)
QWEN_API_KEY="sk-your-qwen-api-key-here"
ANTHROPIC_API_KEY="sk-ant-your-anthropic-key-here"
OPENAI_API_KEY="sk-your-openai-key-here"
